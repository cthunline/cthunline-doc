{
    "name": "cthunline-doc",
    "version": "1.5.1",
    "private": true,
    "scripts": {
        "docusaurus": "docusaurus",
        "start": "docusaurus start",
        "build": "docusaurus build",
        "swizzle": "docusaurus swizzle",
        "deploy": "docusaurus deploy",
        "clear": "docusaurus clear",
        "serve": "docusaurus serve",
        "write-translations": "docusaurus write-translations",
        "write-heading-ids": "docusaurus write-heading-ids",
        "typecheck": "tsc",
        "prepare": "[ -d '.husky' ] && husky || true",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=0 && prettier --check . && stylelint \"**/*.css\" --max-warnings=0",
        "format": "eslint . --ext .js,.jsx,.ts,.tsx --fix --max-warnings=0 && prettier --write . && stylelint \"**/*.css\" --fix --max-warnings=0"
    },
    "dependencies": {
        "@docusaurus/core": "3.4.0",
        "@docusaurus/preset-classic": "3.4.0",
        "@mdx-js/react": "^3.0.1",
        "@svgr/webpack": "^8.1.0",
        "clsx": "^2.1.1",
        "file-loader": "^6.2.0",
        "prism-react-renderer": "^2.3.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "url-loader": "^4.1.1"
    },
    "devDependencies": {
        "@docusaurus/module-type-aliases": "3.4.0",
        "@tsconfig/docusaurus": "^2.0.3",
        "@typescript-eslint/eslint-plugin": "^7.11.0",
        "@typescript-eslint/parser": "^7.11.0",
        "eslint": "^8.57.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^18.0.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-react": "^7.34.2",
        "eslint-plugin-react-hooks": "^4.6.2",
        "husky": "^9.0.11",
        "prettier": "^3.3.0",
        "stylelint": "^16.6.1",
        "stylelint-config-standard": "^36.0.0",
        "stylelint-order": "^6.0.4",
        "typescript": "^5.4.5"
    },
    "browserslist": {
        "production": [
            ">0.5%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
